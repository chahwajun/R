x1 <- function(x) {
  x^2 -1}
x1(2)






my_birthday <- make_date(year = 1996, month = 4, day = 29)


calculate <- function(x){
  (today() - x)/365
}

korean <- function(x){
  (today() - x)/365 +1
}

calculate(my_birthday)

korean(my_birthday)


sister <- make_date(year = 1994, month = 4, day = 24)

calculate(sister)


x111 <- function(var) {
  mtcars |> 
    summarise(mean = mean({{var}}))
}


mtcars |> 
  summarise(mean = mean(cyl))
mtcars |> 
  summarise(mean = mean(cyl))
mtcars |> 
  summarise(mean = mean(cyl))

x111(cyl)

